{
  "name": "alebediev/sorter",
  "description": "Library for sorting sequential and associative arrays of integers and strings",
  "keywords": ["sort", "arrays", "string", "integer"],
  "type": "library",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Andrii Lebediev",
      "email": "wasaby.stnc@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "Alebediev\\Sorter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alebediev\\Sorter\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes",
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --dry-run"
  }
}
